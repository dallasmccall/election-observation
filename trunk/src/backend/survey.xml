		<shortName>Voter Assistance</shortName>
		<leftHeader>Assistance is available</leftHeader>
		<rightHeader># of people</rightHeader>
		<radio>
			<choice>Yes</choice>
			<choice>Don't know</choice>
			<choice>No</choice>	
		</radio>
	</question>
	
